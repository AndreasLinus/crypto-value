<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1235 650">
    <rect width="1235" height="650" fill="#B22234"/>

    <!-- White Stripes -->
    <g fill="#FFFFFF">
        <rect width="1235" height="50" y="50"/>
        <rect width="1235" height="50" y="150"/>
        <rect width="1235" height="50" y="250"/>
        <rect width="1235" height="50" y="350"/>
        <rect width="1235" height="50" y="450"/>
        <rect width="1235" height="50" y="550"/>
    </g>

    <!-- Blue Field -->
    <rect width="494" height="350" fill="#3C3B6E"/>

    <!-- Stars -->
    <g fill="#FFFFFF">
        <!-- 9 rows alternating 6 and 5 stars -->
        <g id="srow1"> <g id="s6">
            <circle cx="41" cy="30" r="15"/>
            <circle cx="123" cy="30" r="15"/>
            <circle cx="205" cy="30" r="15"/>
            <circle cx="287" cy="30" r="15"/>
            <circle cx="369" cy="30" r="15"/>
            <circle cx="451" cy="30" r="15"/>
        </g></g>
        <use href="#srow1" y="60"/>
        <use href="#srow1" y="120"/>
        <use href="#srow1" y="180"/>
        <use href="#srow1" y="240"/>
        <g id="srow2"> <g id="s5">
            <circle cx="82" cy="60" r="15"/>
            <circle cx="164" cy="60" r="15"/>
            <circle cx="246" cy="60" r="15"/>
            <circle cx="328" cy="60" r="15"/>
            <circle cx="410" cy="60" r="15"/>
        </g></g>
        <use href="#srow2" y="60"/>
        <use href="#srow2" y="120"/>
        <use href="#srow2" y="180"/>
        <use href="#srow2" y="240"/>
    </g>
</svg>
